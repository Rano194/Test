Status.AnchorPoint = Vector2.new(0.5, 0.5)
Status.Text = "Status: Collecting coin (1 / 40)"
Status.TextColor3 = Color3.new(1, 1, 1)
Status.TextSize = 38
Status.FontFace = Font.new("rbxasset://fonts/families/FredokaOne.json", Enum.FontWeight.Regular, Enum.FontStyle.Normal)
Status.TextScaled = true
Status.TextWrapped = true
Status.Parent = Main2

task.spawn(function()
    while true do
        task.wait(1)
        if Autofarm_Enable == false then
            local SecondsPassed = game:GetService("ReplicatedStorage").Remotes.Extras.GetTimer:InvokeServer()
            if SecondsPassed > 1 then
                local minutes = math.floor(SecondsPassed / 60)
                local seconds = SecondsPassed % 60
                local formattedTime = string.format("%02d:%02d", minutes, seconds)
                Status.Text = "Status: Wait end game " .. formattedTime
            end
        end
    end
end)

task.spawn(function()
    local data = game:GetService("ReplicatedStorage").Remotes.Extras.GetData2:InvokeServer()
    PassLevel.Text = "Pass-Level: " .. data.Summer2025.CurrentTier
end)
